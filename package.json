{
  "name": "banshop",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "dev:ssr": "ng run store:serve-ssr",
    "serve:ssr": "node dist/store/server/main.js",
    "build:ssr": "ng build && ng run store:server",
    "prerender": "ng run store:prerender",
    "api:typeorm": "ts-node -P apps/api/tsconfig.app.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config apps/api/src/app/config/ormconfig.migrations.ts",
    "api:migration:generate": "npm run api:typeorm migration:generate -- -n",
    "api:migration:create": "npm run api:typeorm migration:create -- -n",
    "api:migration:run": "npm run api:typeorm migration:run",
    "api:migration:revert": "npm run api:typeorm migration:revert",
    "api:seed:config": "ts-node -P apps/api/tsconfig.app.json -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js -n apps/api/src/app/config/ormconfig.migrations.ts config",
    "api:seed:run": "ts-node -P apps/api/tsconfig.app.json -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js -n apps/api/src/app/config/ormconfig.migrations.ts seed"
  },
  "private": true,
